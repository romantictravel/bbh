<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
	http://code.alibabatech.com/schema/dubbo http://code.alibabatech.com/schema/dubbo/dubbo.xsd">

    <dubbo:application name="bbh-basic-provider"  logger="slf4j"  owner="tc"/>
    <!--
    <dubbo:registry address="zookeeper://192.168.1.190:2181"/>
    -->
    <dubbo:registry address="zookeeper://192.168.1.141:2182"/>

    <dubbo:protocol  name="dubbo" port="20880" accesslog="true"/>

    <bean id="userServiceImpl" class="com.bbh.basic.povider.service.UserServiceImpl" />

    <dubbo:service interface="com.bbh.basic.api.UserService" ref="userServiceImpl"/>

</beans>