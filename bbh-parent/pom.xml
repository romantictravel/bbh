    <dependencyManagement>

        <dependencies>
            <!--<dependency>
                <groupId>org.springframework.boot</groupId>
                <artifactId>spring-boot-autoconfigure</artifactId>
                <version>1.2.x</version>
            </dependency>-->
            <!--spring  begin-->
            <dependency>
                <groupId>org.springframework.data</groupId>
